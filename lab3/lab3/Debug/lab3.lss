                                 .def  a = r11
                                 .def  x_ = r12
                                 .def  b = r13
                                 .def  input_val = r15  ; Register for storing key input value
                                 .def  row = r16        ; Current row register
                                 .def  col = r17        ; Current column register
                                 .def  rmask = r18      ; Mask for row scanning
                                 .def  cmask = r19      ; Mask for column scanning
                                 .def  temp1 = r20      ; Temporary register 1
                                 .def  temp2 = r21      ; Temporary register 2
                                 .def  total = r23      ; Register for storing accumulated keypad input
                                 .def  y_high = r24     ; Higher byte of the result
                                 .def  y_low = r25      ; Lower byte of the result
                                 .equ PORTLDIR = 0xF0   ; PL7-4 for output, PL3-0 for input
                                 .equ ROWMASK = 0x0F    ; Used to get row input from Port L
                                 .equ INITCOLMASK = 0xEF  ; Initial column mask, scanning from left to right
                                 .equ INITROWMASK = 0x01  ; Initial row mask, scanning from top to bottom
                                 .equ symbol1 = 0x2A     ; ASCII value for '*'
                                 .equ symbol2 = 0x44     ; ASCII value for 'D'
                                 .equ symbol3 = 0x23     ; ASCII value for '#'
                                 ; LCD section
                                 ; LCD commands
                                 .equ LCD_FUNC_SET  =0b00111000 ; 001 + DL(8 bits) N(2 line) F(5x7 dots)
                                 .equ LCD_DSPL_OFF  =0b00001000 ; 00001 + OFF + 00
                                 .equ LCD_DSPL_ON   =0b00001111 ; 00001 + ON C(show cursor) B(do blink)
                                 .equ LCD_DSPL_CLR  =0b00000001 ; clears display and moves cursor to top-left
                                 .equ LCD_ENTRY_SET =0b00000110 ; 000001 + I/D(increment) + S(no shift)
                                 ; LCD bits
                                 .equ LCD_RS=7  ; bit of RS
                                 .equ LCD_RW=5  ; bit of RW
                                 .equ LCD_E=6   ; bit of E
                                 .equ LCD_BF=7  ; bit of BF
                                 .def data=r22  ; register to store LCD data and keypad input data
                                 
                                 ; Macro: Delay routine
                                 .macro delay_macro
                                     ldi temp1, 0xFF
                                 delay_loop:
                                     dec temp1
                                     brne delay_loop
                                 .endmacro
                                 
                                 ; Macro: controlable delay 1 iteration = 1ns delay
                                 ; number of iterations is specified in r27:r26
                                 .macro delayControllable
                                 	outerLoop:
                                 		ldi temp1, 3
                                 		innerLoop:
                                 			dec temp1
                                 			nop
                                 			brne innerLoop
                                 		subi r26, 1
                                 		sbci r27, 0
                                 		brne outerLoop
                                 .endmacro
                                 
                                 ; computes hex code of a single digit for LCD
                                 .macro getHexCode
                                 	cpi @0, 10
                                 	brlo decNum    ; if digit < 10, dispkya a number 0-9
                                 	subi @0, 10    ; otherwise, display a letter A-F;
                                 	subi @0, -'A'  ; subtract 10 from the digit and add ASCII code of 'A'
                                 	rjmp endGetHexCode
                                 	decNum:
                                 		subi @0, -'0' ; add hex code of '0' to the number
                                 	endGetHexCode:
                                 .endmacro
                                 
                                 ; Macro: Handle keypad input
                                 .macro keypad_input_macro 
                                     clr total               ; Clear accumulated value
                                 keypad_input_loop:
                                     rcall get_keypad_input  ; Call subroutine to get keypad input
                                     mov temp1, input_val       ; Read the input number
                                     ; Check if the input is the end character
                                     cpi temp1, @0
                                     breq end_keypad_input
                                 	; write the digit to LCD
                                 	mov data, temp1
                                 	subi data, -'0' ; convert to ASCII by adding code of '0'
                                 	rcall lcd_write_data
                                 	rcall lcd_wait_busy
                                     ldi temp2, 10
                                     mul total, temp2           ; total * 10
                                     mov total, r0
                                     add total, temp1           ; total += current input value
                                     rjmp keypad_input_loop
                                 end_keypad_input:
                                 	cpi temp1, 'D'
                                 	brne end_keypad_input_cont
                                 	ldi temp1, '-'
                                 end_keypad_input_cont:
                                 	mov data, temp1   ; write the character to LCD
                                 	rcall lcd_write_data
                                 	rcall lcd_wait_busy
                                     mov @1, total  ; Store the input value in the specified register
                                 .endmacro
                                 
                                 ; Macro: Handle keypad input
                                 .macro keypad_wait_for_C_and_A
                                 keypad_wait_loop:
                                     rcall get_keypad_input  ; Call subroutine to get keypad input
                                     mov temp1, input_val     ; Read the input
                                     ; Check if the input is 'C'
                                     cpi temp1, 'C'
                                     breq convert_display
                                 	; Check if the input is 'A'
                                     cpi temp1, 'A'
                                     breq do_restart
                                     rjmp keypad_wait_loop
                                 convert_display:
                                     ldi @0, 1
                                 	rjmp end_keypad_wait_macro
                                 do_restart:
                                 	ldi @0, 2
                                 end_keypad_wait_macro:
                                 .endmacro
                                 
                                 ; Macro: Convert the row and column input to the actual value
                                 .macro convert_macro
                                     cpi col, 3                    ; If column is 3, we have a letter
                                     breq letters
                                     cpi row, 3                    ; If row is 3, we have a symbol or 0
                                     breq symbols
                                     mov temp1, row                ; Otherwise, we have a number from 1-9
                                     lsl temp1
                                     add temp1, row                ; temp1 = row * 3
                                     add temp1, col                ; Add column value to get the key value
                                 	inc temp1                     ; Increment number to move it from 0-8 to 1-9
                                     jmp convert_end
                                 letters:
                                     ldi temp1, 'A'
                                     add temp1, row                ; Increment character 'A' by the row value
                                     jmp convert_end
                                 symbols:
                                     cpi col, 0                    ; Check if we have '*'
                                     breq star
                                     cpi col, 1                    ; Check if we have '0'
                                     breq zero
                                     ldi temp1, '#'                ; Otherwise, it's '#'
                                     jmp convert_end
                                 star:
                                     ldi temp1, '*'                ; Set value to '*'
                                     jmp convert_end
                                 zero:
                                     ldi temp1, 0                  ; Set value to 0
                                 convert_end:
                                 	mov input_val, temp1          ; Output the key value to input_val variable
                                 .endmacro
                                 
